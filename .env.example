# Copy this file to .env and fill in your actual values

# Django secret key - generate a new one for production
# You can generate one using: python3 -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"
SECRET_KEY=your-secret-key-here

# Set to False in production
DEBUG=True

# Comma-separated list of allowed hosts (leave empty for development)
# Example for production: ALLOWED_HOSTS=yourdomain.com,www.yourdomain.com
ALLOWED_HOSTS=